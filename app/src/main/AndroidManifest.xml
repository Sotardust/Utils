<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dai.recycler">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sqlite.SQLiteActivity" />
        <activity android:name=".file.FileActivity" />
        <activity android:name=".RecyclerActivity" />
        <activity android:name=".rollview.RollActivity" />
        <activity android:name=".service.ServiceActivity" />
        <activity android:name=".aidl.AidlActivity" />
        <activity android:name=".broadcast.BroadCastActivity" />
        <activity android:name=".view.bookpageview.BookPageViewActivity" />
        <activity android:name=".view.ViewActivity" />
        <activity android:name=".view.fallingview.FallingActivity" />
        <activity android:name=".view.musicbutton.MusicActivity" />

        <service android:name=".service.MyService" />
        <service
            android:name=".service.MyService2"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name=".aidl.MyService"
            android:enabled="true"
            android:exported="true"></service>
    </application>

</manifest>